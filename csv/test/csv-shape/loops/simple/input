basic[0]="seq 1 10 | csv-shape loop -n 2 | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"
basic[1]="seq 1 10 | csv-shape loop -n 3 | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"
basic[2]="seq 1 10 | csv-shape loop -n 9 | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"
basic[3]="seq 1 10 | csv-shape loop -n 10 | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"
basic[4]="seq 1 10 | csv-shape loop -n 11 | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"
basic[5]="seq 1 10 | csv-shape loop -n 1  | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"


binary[0]="seq 1 10 | csv-to-bin i | csv-shape loop -n 2  --binary i | csv-from-bin 2i | tr \\\\n ';' | sed 's#;$##';  (( ${PIPESTATUS[2]} == 0 ))"
binary[1]="seq 1 10 | csv-to-bin i | csv-shape loop -n 3  --binary i | csv-from-bin 3i | tr \\\\n ';' | sed 's#;$##';  (( ${PIPESTATUS[2]} == 0 ))"
binary[2]="seq 1 10 | csv-to-bin i | csv-shape loop -n 9  --binary i | csv-from-bin 9i | tr \\\\n ';' | sed 's#;$##';  (( ${PIPESTATUS[2]} == 0 ))"
binary[3]="seq 1 10 | csv-to-bin i | csv-shape loop -n 10 --binary i | csv-from-bin 10i | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[2]} == 0 ))"
binary[4]="seq 1 10 | csv-to-bin i | csv-shape loop -n 11 --binary i | csv-from-bin 11i | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[2]} == 0 ))"
binary[5]="seq 1 10 | csv-to-bin i | csv-shape loop -n 1  --binary i | csv-from-bin i | tr \\\\n ';' | sed 's#;$##';   (( ${PIPESTATUS[2]} == 0 ))"


multi_fields[0]="( echo 1,a,2; echo 2,b,u; echo b,b,b; echo 5,d,6 ) | csv-shape loop -n 2 | tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[1]} == 0 ))"

multi_fields_binary[0]="( echo 1,a,2; echo 2,b,7; echo 3,c,4; echo 5,d,6; ) | csv-to-bin 'ui,s[1],ui' | csv-shape loop -n 3 --binary 'ui,s[1],ui' | csv-from-bin 'ui,s[1],ui,ui,s[1],ui,ui,s[1],ui'| tr \\\\n ';' | sed 's#;$##'; (( ${PIPESTATUS[2]} == 0 ))"
