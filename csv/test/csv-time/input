legacy[1]="echo 20140305T230000,20140305T230000,20140305T230000 | csv-time --from iso --to seconds --fields 2,3"
legacy[2]="echo 20140305T230000,blue,20140305T230000 | csv-time --from iso --to seconds --fields 3"

